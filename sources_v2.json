        "Method": "GET",
        "ContentType": "application/json",
        "Url": "https://api.dynadot.com/api3.xml?key=8H7z8Y9G8R6p6k6dbQ9R6Z6MBsgN8i6m8O7U7pC7u&command=search&domain0={domain}",
        "IsXml": true,
        "PathCheck": "$.Results.SearchResponse.SearchHeader.Available",
        "ValueCheck": "yes",
