[
  {
    "Name": "1Byte",
    "Steps": [
      {
        "Method": "GET",
        "ContentType": "application/json",
        "Url": "https://api.1byte.com/domains/search-iu?domain={domain}&page=1",
        "PathCheck": "$.domain.available",
        "ValueCheck": "true",
        "UnsupportedCondition": "CONTAINT",
        "UnsupportedValue": "{\"domain\":null"
      }
    ]    
  },
  {
    "Name": "Gandi",
    "Steps": [
      {
        "Method": "GET",
        "ContentType": "application/json",
        "Header": {
          "Authorization": "Apikey JE1DEpsSLdBYpwwAjmemD4pA"
        },
        "Url": "https://api.gandi.net/v5/domain/check?name={domain}",
        "PathCheck": "$.products[0].status",
        "ValueCheck": "available",
        "UnsupportedCondition": "NOT_CONTAINT",
        "UnsupportedValue": "{domain}"
      }
    ]    
  },
  {
    "Name": "Secureserver",
    "Steps": [
      {
        "Method": "GET",
        "ContentType": "application/json",
        "Url": "https://www.secureserver.net/api/v1/search/exact?plid=1&q={domain}",
        "PathCheck": "$.ExactMatchDomain.AvailabilityLabel",
        "ValueCheck": "available_for_registration",
        "AvailableCondition": "CONTAINT",
        "AvailableValue": "\"Fqdn\":\"{domain}\"",
        "UnsupportedCondition": "CONTAINT",
        "UnsupportedValue": "\"Message\":\"The tld you are searching for is not available"
      }
    ]
  },
]
